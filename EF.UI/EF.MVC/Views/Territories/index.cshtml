@model List<EF.MVC.Models.TerritoriesView>

@{ 
    ViewBag.Title = "Territories page";
}

<h1>Lista de territorios</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Descripcion</th>
            <th scope="col">ID Region</th>
        </tr>
    </thead>
    <tbody>
    </tbody>

        @foreach (var territories in Model)
        {
            <tr>
                <td scope="row">@territories.Id</td>
                <td>@territories.Descripcion</td>
                <td scope="row">@territories.RegionId</td>
            </tr>
        }

</table>

